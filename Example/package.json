{
  "name": "palette-mobile-example",
  "version": "22.9.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "android": "expo run:android --port 8082",
    "beta:ios": "yarn sync:example-version && eas build --platform ios --profile beta --local --output build.ipa && eas submit --platform ios --profile beta --path build.ipa",
    "beta:ios:ci": "gh workflow run build-ios.yml -r $(git branch --show-current)",
    "update:beta": "eas update --branch beta",
    "ios": "expo run:ios --port 8082",
    "start": "npx expo start --port 8082",
    "start:reset-cache": "npx expo start -c --port 8082",
    "android:prebuild": "npx expo prebuild --clean --platform android",
    "ios:prebuild": "npx expo prebuild --clean --platform ios",
    "prestart": "sb-rn-get-stories",
    "storybook-watcher": "sb-rn-watcher",
    "sync:example-version": "node ../scripts/sync-example-version.js"
  },
  "dependencies": {
    "@artsy/palette-mobile": "*",
    "@d11/react-native-fast-image": "8.12.0",
    "@gorhom/bottom-sheet": "5.1.8",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/slider": "5.0.1",
    "@storybook/addon-ondevice-actions": "^8.3.5",
    "@storybook/addon-ondevice-controls": "^8.3.5",
    "@storybook/react-native": "^8.6.2",
    "expo": "54.0.20",
    "expo-build-properties": "~1.0.10",
    "expo-dev-client": "6.0.16",
    "expo-font": "14.0.9",
    "expo-updates": "~29.0.16",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-blurhash": "2.1.2",
    "react-native-collapsible-tab-view": "patch:react-native-collapsible-tab-view@npm%3A9.0.0-rc.0#~/.yarn/patches/react-native-collapsible-tab-view-npm-9.0.0-rc.0-5363b327de.patch",
    "react-native-device-info": "14.0.4",
    "react-native-gesture-handler": "2.28.0",
    "react-native-haptic-feedback": "1.14.0",
    "react-native-linear-gradient": "2.6.2",
    "react-native-pager-view": "7.0.1",
    "react-native-popover-view": "^6.1.0",
    "react-native-reanimated": "4.2.1",
    "react-native-safe-area-context": "5.6.0",
    "react-native-svg": "15.12.1",
    "react-native-worklets": "0.7.2",
    "styled-components": "6.1.19"
  },
  "devDependencies": {
    "@types/react": "~19.1.10",
    "patch-package": "^8.0.1",
    "typescript": "5.9.2"
  },
  "resolutions": {
    "metro-config": "0.81.5",
    "babel-preset-expo": "14.0.0"
  },
  "reanimated": {
    "staticFeatureFlags": {
      "USE_COMMIT_HOOK_ONLY_FOR_REACT_COMMITS": true,
      "DISABLE_COMMIT_PAUSING_MECHANISM": true,
      "ANDROID_SYNCHRONOUSLY_UPDATE_UI_PROPS": true,
      "IOS_SYNCHRONOUSLY_UPDATE_UI_PROPS": true
    }
  }
}
