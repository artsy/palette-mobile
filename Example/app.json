{
  "expo": {
    "name": "Palette Mobile",
    "slug": "palettemobile",
    "version": "22.9.0",
    "splash": {
      "image": "./assets/icons/app_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "net.artsy.palette-mobile",
      "supportsTablet": true,
      "icon": "./assets/icons/app_icon.png",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "net.artsy.palette_mobile",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/app_icon_android.png",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Unica77LL-Italic.otf",
            "./assets/fonts/Unica77LL-MediumItalic.otf",
            "./assets/fonts/Unica77LL-Medium.otf",
            "./assets/fonts/Unica77LL-Regular.otf"
          ]
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 36,
            "targetSdkVersion": 36,
            "minSdkVersion": 24,
            "ndkVersion": "27.1.12297006",
            "kotlinVersion": "2.1.20",
            "gradleProperties": {
              "org.gradle.jvmargs": "-Xmx4096m -XX:MaxMetaspaceSize=1024m -Dkotlin.daemon.jvm.options=-Xmx2048m -Dfile.encoding=UTF-8",
              "org.gradle.parallel": "true"
            }
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "9dda6017-e129-435c-aad8-3ece8b207ee9"
      }
    },
    "owner": "artsy_org",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/9dda6017-e129-435c-aad8-3ece8b207ee9"
    }
  }
}
